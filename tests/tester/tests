[100]
NAME="Separators"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls; ls -la ; ls"
[100-END]

[101]
NAME="simple pipe"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls | grep test"
[101-END]

[102]
NAME="big pipe"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls | grep test"
  echo "cat big.txt| grep www"
[102-END]

[103]
NAME="multiple pipe"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls .. | cat | grep main"
[103-END]

[104]
NAME="redirection"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls > aa"
  echo "cat aa"
  echo "rm aa"
[104-END]

[105]
NAME="redirection + pipe"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls > aa"
  echo "cat <aa | wc"
  echo "rm aa"
[105-END]

[106]
NAME="redirection stdout"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls > out; echo test >> out"
  echo "cat out"
  echo "rm out"
[106-END]

[200]
NAME="basic inhibitor"
